def getAddressHash(field[2] pubKey) -> (field[2]):
    a = unpack256(pubKey[0])
    b = unpack256(pubKey[1])
   
    field[256] lhs = a
    field[256] rhs = b
    
    field[256] r = sha256(lhs, rhs)
    
    res0 = pack128(r[..128])
    res1 = pack128(r[128..])

    return [res0, res1]

