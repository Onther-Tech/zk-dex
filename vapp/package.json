{
  "name": "vapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "truffle test",
    "lint": "eslint --ext .js,.vue src",
    "compile": "truffle compile",
    "initialize:zokrates": "docker exec -it --workdir /home/zokrates/circuit zokrates /bin/bash /home/zokrates/circuit/zk-initialize.sh",
    "lint:fix": "solium -d contracts/ --fix",
    "migrate": "truffle migrate --reset",
    "start:zokrates": "docker run -d -it --name zokrates -v $(pwd)/circuit:/home/zokrates/circuit -v $(pwd)/contracts:/home/zokrates/contracts zokrates/zokrates:latest /bin/bash",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "apexcharts": "^3.8.6",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "core-js": "^2.6.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "keythereum": "^1.0.4",
    "node-localstorage": "^1.3.1",
    "rlp": "^2.2.3",
    "truffle-contract": "^4.0.25",
    "vue": "^2.6.10",
    "vue-apexcharts": "^1.5.0",
    "vue-router": "^3.0.7",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1",
    "web3": "^1.0.0-beta.37"
  },
  "devDependencies": {
    "@vue/babel-preset-app": "^3.9.2",
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.9.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "bootstrap-vue": "^2.0.0-rc.28",
    "eslint": "^5.16.0",
    "eslint-plugin-es-beautifier": "^1.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "web3-utils": "^1.0.0"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
