{
  "name": "vapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "compile": "truffle compile",
    "migrate": "truffle migrate --reset",
    "server": "DEBUG=* node server",
    "test": "truffle test",
    "test:server": "npx jest ./app.test.js --bail",
    "test:server:debug": "node --inspect-brk node_modules/.bin/jest ./app.test.js --bail --runInBand --forceExit --detectOpenHandles",
    "test:server:debug:watch": "node --inspect-brk node_modules/.bin/jest ./app.test.js --bail --runInBand --watch",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "solium -d contracts/ --fix",
    "start:zokrates": "docker run -d -it --name zokrates -v $(pwd)/circuit:/home/zokrates/circuit -v $(pwd)/contracts:/home/zokrates/contracts zokrates/zokrates:latest /bin/bash",
    "initialize:zokrates": "docker exec -it --workdir /home/zokrates/circuit zokrates /bin/bash /home/zokrates/circuit/zk-initialize.sh"
  },
  "dependencies": {
    "async": "^3.1.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "buefy": "^0.8.2",
    "core-js": "^2.6.5",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "jsonic": "^0.3.1",
    "keythereum": "^1.0.4",
    "moment": "^2.24.0",
    "node-localstorage": "^1.3.1",
    "rlp": "^2.2.3",
    "truffle-contract": "^4.0.30",
    "vue": "^2.6.10",
    "vue-router": "^3.0.7",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1",
    "web3": "^1.2.1"
  },
  "devDependencies": {
    "@vue/babel-preset-app": "^3.9.2",
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-es-beautifier": "^1.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "ganache-cli": "^6.6.0",
    "jest": "^24.9.0",
    "lodash": "^4.17.15",
    "moxios": "^0.4.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "supertest": "^4.0.2",
    "web3-utils": "^1.2.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
