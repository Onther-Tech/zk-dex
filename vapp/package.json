{
  "name": "vapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "compile": "truffle compile",
    "migrate": "truffle migrate --reset",
    "test": "truffle test",
    "lint": "solium --dir ./contracts",
    "lint:fix": "solium -d contracts/ --fix",
    "start:zokrates": "docker run -d -it --name zokrates -v $(pwd)/circuit:/home/zokrates/circuit -v $(pwd)/contracts:/home/zokrates/contracts zokrates/zokrates:latest /bin/bash",
    "initialize:zokrates": "docker exec -it --workdir /home/zokrates/circuit zokrates /bin/bash /home/zokrates/circuit/zk-initialize.sh"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "core-js": "^2.6.5",
    "dockerode": "^2.5.8",
    "eccrypto": "^1.1.1",
    "element-ui": "^2.10.1",
    "ethlint": "^1.2.4",
    "jsonic": "^0.3.1",
    "rlp": "^2.2.3",
    "secp256k1": "^3.7.1",
    "truffle": "^5.0.28",
    "vue": "^2.6.10",
    "vue-router": "^3.0.7",
    "vuex": "^3.1.1",
    "web3": "^0.20.7"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
